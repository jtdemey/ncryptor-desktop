{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "ncryptor",
  "mainBinaryName": "ncryptor",
  "version": "0.7.0",
  "identifier": "com.jt.ncryptor",
  "plugins": {},
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Ncryptor",
        "width": 1280,
        "height": 720,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
